{
    "name": "My Development Environment",
    "extensions": [
      "ms-python.python",
      "dbaeumer.vscode-eslint",
      "ms-vscode-remote.remote-containers"
    ],
    "containerEnv": {
      "USE_WSL": "false",
      "NODE_ENV": "development"
    },
    "mounts": [
      "source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind"
    ],
    "remoteUser": "node",
    "appPort": 3000,
    "postCreateCommand": "npm install"
  }
  